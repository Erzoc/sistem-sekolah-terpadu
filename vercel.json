{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "env": {
    "NEXTAUTH_URL": "@NEXTAUTH_URL",
    "NEXTAUTH_SECRET": "@NEXTAUTH_SECRET",
    "DATABASE_URL": "@DATABASE_URL",
    "GOOGLE_GENERATIVE_AI_API_KEY": "@GOOGLE_GENERATIVE_AI_API_KEY"
  },
  "functions": {
    "src/app/api/**/*.ts": {
      "memory": 256,
      "maxDuration": 30
    }
  }
}
